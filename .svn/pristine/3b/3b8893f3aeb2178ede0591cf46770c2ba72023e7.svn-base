package kr.or.fms.hq.dto;

public class BuyGoodsVO {

	private String goods_pur_code;	//물품구매코드
	private String hq_code;
	private String goods_pur_date;	//구매일자
	private String goods_pur_price;	//총구매금액
	private String goods_pur_st;	//구매승인상태
	
	public String getGoods_pur_code() {
		return goods_pur_code;
	}
	public void setGoods_pur_code(String goods_pur_code) {
		this.goods_pur_code = goods_pur_code;
	}
	public String getHq_code() {
		return hq_code;
	}
	public void setHq_code(String hq_code) {
		this.hq_code = hq_code;
	}
	public String getGoods_pur_date() {
		return goods_pur_date;
	}
	public void setGoods_pur_date(String goods_pur_date) {
		this.goods_pur_date = goods_pur_date;
	}
	public String getGoods_pur_price() {
		return goods_pur_price;
	}
	public void setGoods_pur_price(String goods_pur_price) {
		this.goods_pur_price = goods_pur_price;
	}
	public String getGoods_pur_st() {
		return goods_pur_st;
	}
	public void setGoods_pur_st(String goods_pur_st) {
		this.goods_pur_st = goods_pur_st;
	}
	
	
}
